{
    "name": "eslint-config-paysera",
    "version": "0.0.1",
    "description": "Paysera's ESLint config, following our styleguide",
    "main": "index.js",
    "scripts": {
        "prelint": "editorconfig-tools check * rules/* test/*",
        "lint": "eslint .",
        "prepublish": "(in-install || eslint-find-rules --unused) && not-in-publish && safe-publish-latest"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/paysera/js-style-guide"
    },
    "keywords": [
        "eslint",
        "eslintconfig",
        "config",
        "paysera",
        "javascript",
        "styleguide"
    ],
    "author": "Paysera (https://www.paysera.com)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/paysera/js-style-guide/issues"
    },
    "homepage": "https://github.com/paysera/js-style-guide",
    "dependencies": {
        "eslint-config-paysera-base": "^0.0.1",
        "eslint-config-airbnb": "^16.0.1"
    },
    "devDependencies": {
        "editorconfig-tools": "^0.1.1",
        "eslint": "^4.18.1",
        "eslint-find-rules": "^3.2.0",
        "eslint-plugin-import": "^2.9.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "eslint-plugin-react": "^7.7.0",
        "in-publish": "^2.0.0",
        "react": ">= 0.13.0",
        "safe-publish-latest": "^1.1.1"
    },
    "peerDependencies": {
        "eslint": "^4.18.1",
        "eslint-plugin-import": "^2.9.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "eslint-plugin-react": "^7.7.0"
    },
    "engines": {
        "node": ">= 4"
    }
}
