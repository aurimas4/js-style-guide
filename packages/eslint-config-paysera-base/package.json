{
    "name": "eslint-config-paysera-base",
    "version": "0.0.1",
    "description": "Paysera's base JS ESLint config, following our styleguide",
    "main": "index.js",
    "scripts": {
        "prelint": "editorconfig-tools check * rules/*",
        "lint": "eslint --report-unused-disable-directives .",
        "prepublish": "(in-install || eslint-find-rules --unused) && not-in-publish && safe-publish-latest"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/paysera/js-style-guide"
    },
    "keywords": [
        "eslint",
        "eslintconfig",
        "config",
        "paysera",
        "javascript",
        "styleguide"
    ],
    "author": "Paysera (https://www.paysera.com)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/paysera/js-style-guide/issues"
    },
    "homepage": "https://github.com/paysera/js-style-guide",
    "devDependencies": {
        "editorconfig-tools": "^0.1.1",
        "eslint": "^4.18.1",
        "eslint-find-rules": "^3.2.0",
        "eslint-plugin-import": "^2.9.0",
        "in-publish": "^2.0.0",
        "safe-publish-latest": "^1.1.1"
    },
    "peerDependencies": {
        "eslint": "^4.18.1",
        "eslint-plugin-import": "^2.9.0"
    },
    "engines": {
        "node": ">= 4"
    },
    "dependencies": {
        "eslint-config-airbnb-base": "^12.1.0"
    }
}
